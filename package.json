{
  "name": "angular2-mdl",
  "version": "0.0.0",
  "description": "Angular 2-6 components, directives and styles based on material design lite https://getmdl.io.",
  "keywords": [],
  "homepage": "http://mseemann.io/angular2-mdl/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mseemann/angular2-mdl.git"
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.js --progress",
    "build:clean": "rimraf dist",
    "build:dist": "npm run build:clean && npm run build:umd && npm run build:esm && npm run build:scss && cp src/lib/package.json dist && cp README.md dist",
    "build:esm": "ngc -p src/lib/tsconfig-ngc.json",
    "build:scss": "ncp src/lib/scss dist/scss",
    "build:umd": "webpack --config config/webpack/webpack.config.umd.js --progress",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=160aba8aa0baa5d0cab1d2162f50027cef9cda032fbc4eebefd54167ddce4c89 codeclimate-test-reporter < coverage/lcov.info",
    "deploy-gh-pages": "npm run build && ./scripts/deploy.sh --message \"Deploy gh-pages from commit $(git rev-parse HEAD)  [ci skip]\" -n",
    "pree2e": "webdriver-manager update; tsc --project e2e",
    "e2e": "protractor config/protractor.sauce.conf.js",
    "e2e:local": "protractor config/protractor.conf.js",
    "generate-scss-from-mdl-scss": "rimraf src/lib/scss/mdl && node ./scripts/generate-scss-from-mdl-scss.js",
    "start": "webpack-dev-server",
    "start:dist": "rimraf dist && webpack --config config/webpack/webpack.prod.js --progress && http-server dist -a localhost -p 4200",
    "start:dist:dev": "rimraf dist && webpack --config config/webpack/webpack.dev.js --progress && http-server dist -a localhost -p 4200",
    "test": "karma start config/karma.conf.js",
    "test:watch": "karma start config/karma.conf.js --watch",
    "pre-push": "npm run test && npm run build:dist"
  },
  "dependencies": {
    "@angular/animations": "6.1.9",
    "@angular/common": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/forms": "6.1.9",
    "webpack-angular-externals": "^1.0.2",
    "webpack-rxjs-externals": "^2.0.0"
  },
  "devDependencies": {
    "@angular/compiler": "6.1.9",
    "@angular/compiler-cli": "6.1.9",
    "@angular/http": "6.1.9",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/platform-server": "6.1.9",
    "@angular/router": "6.1.9",
    "@types/chai": "4.1.4",
    "@types/jasmine": "3.3.13",
    "@types/node": "12.0.2",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "9.5.1",
    "awesome-typescript-loader": "5.2.1",
    "codeclimate-test-reporter": "0.5.1",
    "codelyzer": "4.4.2",
    "copy-webpack-plugin": "5.0.3",
    "core-js": "2.6.5",
    "coveralls": "3.0.3",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "3.0.0",
    "handlebars-loader": "1.7.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "http-server": "^0.11.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-remap-istanbul": "0.6.0",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "3.0.5",
    "material-design-lite": "1.3.0",
    "ncp": "2.0.0",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "protractor": "6.0.0",
    "raw-loader": "2.0.0",
    "rimraf": "2.6.3",
    "rxjs": "6.2.2",
    "sass-loader": "7.1.0",
    "sourcemap-istanbul-instrumenter-loader": "0.2.0",
    "string-replace-loader": "2.1.1",
    "style-loader": "0.23.1",
    "ts-node": "8.2.0",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0",
    "typescript": "2.9.1",
    "web-animations-js": "2.3.1",
    "webpack": "4.32.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "3.4.1",
    "webpack-merge": "4.2.1",
    "zone.js": "0.8.26"
  },
  "optionalDependencies": {
    "fsevents": "2.0.7"
  },
  "private": true
}
