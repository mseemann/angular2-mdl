{
  "name": "angular2-mdl",
  "version": "0.0.0",
  "description": "Angular 2 components, directives and styles based on material design lite https://getmdl.io.",
  "keywords": [],
  "homepage": "http://mseemann.io/angular2-mdl/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mseemann/angular2-mdl.git"
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.js --progress --profile",
    "build:clean": "rimraf dist",
    "build:dist": "npm run build:clean && npm run build:umd && npm run build:esm && npm run build:scss && cp src/lib/package.json dist && cp README.md dist",
    "build:esm": "ngc -p src/lib/tsconfig-ngc.json",
    "build:scss": "ncp src/lib/scss dist/scss",
    "build:umd": "webpack --config config/webpack/webpack.config.umd.js --progress",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=160aba8aa0baa5d0cab1d2162f50027cef9cda032fbc4eebefd54167ddce4c89 codeclimate-test-reporter < coverage/lcov.info",
    "deploy-gh-pages": "npm run build && ./scripts/deploy.sh --message \"Deploy gh-pages from commit $(git rev-parse HEAD)  [ci skip]\" -n",
    "pree2e": "webdriver-manager update; tsc --project e2e",
    "e2e": "protractor config/protractor.sauce.conf.js",
    "e2e:local": "protractor config/protractor.conf.js",
    "generate-scss-from-mdl-scss": "rimraf src/lib/scss/mdl && node ./scripts/generate-scss-from-mdl-scss.js",
    "start": "webpack-dev-server",
    "test": "karma start config/karma.conf.js",
    "test:watch": "karma start config/karma.conf.js --watch"
  },
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3"
  },
  "devDependencies": {
    "@angular/compiler": "2.4.3",
    "@angular/compiler-cli": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/platform-server": "2.4.3",
    "@angular/router": "3.4.3",
    "@types/chai": "3.4.34",
    "@types/jasmine": "2.5.40",
    "@types/node": "7.0.0",
    "angular2-template-loader": "0.6.0",
    "autoprefixer": "6.6.1",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "codeclimate-test-reporter": "0.4.0",
    "codelyzer": "2.0.0-beta.4",
    "copy-webpack-plugin": "4.0.1",
    "core-js": "2.4.1",
    "coveralls": "2.11.15",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "handlebars-loader": "1.4.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.26.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-remap-istanbul": "0.4.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.8.1",
    "material-design-lite": "1.3.0",
    "ncp": "2.0.0",
    "node-sass": "4.2.0",
    "postcss-loader": "1.2.2",
    "protractor": "5.0.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "rxjs": "5.0.3",
    "sass-loader": "4.1.1",
    "sourcemap-istanbul-instrumenter-loader": "0.2.0",
    "string-replace-loader": "1.0.5",
    "style-loader": "0.13.1",
    "ts-node": "2.0.0",
    "tslint": "4.3.1",
    "tslint-loader": "3.3.0",
    "typescript": "2.0.10",
    "typings": "2.1.0",
    "web-animations-js": "2.2.2",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.4",
    "webpack-merge": "2.4.0",
    "zone.js": "0.7.4"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.14"
  },
