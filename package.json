{
  "name": "angular2-mdl",
  "version": "0.0.0",
  "description": "Angular 2 components, directives and styles based on material design lite https://getmdl.io.",
  "keywords": [],
  "homepage": "http://mseemann.io/angular2-mdl/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mseemann/angular2-mdl.git"
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.js --progress",
    "build:clean": "rimraf dist",
    "build:dist": "npm run build:clean && npm run build:umd && npm run build:esm && npm run build:scss && cp src/lib/package.json dist && cp README.md dist",
    "build:esm": "ngc -p src/lib/tsconfig-ngc.json",
    "build:scss": "ncp src/lib/scss dist/scss",
    "build:umd": "webpack --config config/webpack/webpack.config.umd.js --progress",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=160aba8aa0baa5d0cab1d2162f50027cef9cda032fbc4eebefd54167ddce4c89 codeclimate-test-reporter < coverage/lcov.info",
    "deploy-gh-pages": "npm run build && ./scripts/deploy.sh --message \"Deploy gh-pages from commit $(git rev-parse HEAD)  [ci skip]\" -n",
    "pree2e": "webdriver-manager update; tsc --project e2e",
    "e2e": "protractor config/protractor.sauce.conf.js",
    "e2e:local": "protractor config/protractor.conf.js",
    "generate-scss-from-mdl-scss": "rimraf src/lib/scss/mdl && node ./scripts/generate-scss-from-mdl-scss.js",
    "start": "webpack-dev-server",
    "start:dist": "rimraf dist && webpack --config config/webpack/webpack.prod.js --progress && http-server dist -a localhost -p 4200",
    "start:dist:dev": "rimraf dist && webpack --config config/webpack/webpack.dev.js --progress && http-server dist -a localhost -p 4200",
    "test": "karma start config/karma.conf.js",
    "test:watch": "karma start config/karma.conf.js --watch"
  },
  "dependencies": {
    "@angular/animations": "4.4.6",
    "@angular/common": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6"
  },
  "devDependencies": {
    "@angular/compiler": "4.4.6",
    "@angular/compiler-cli": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@angular/platform-server": "4.4.6",
    "@angular/router": "4.4.6",
    "@types/chai": "4.0.4",
    "@types/node": "8.0.53",
    "@types/jasmine": "2.8.2",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "7.1.6",
    "awesome-typescript-loader": "3.3.0",
    "codeclimate-test-reporter": "0.5.0",
    "codelyzer": "4.0.1",
    "copy-webpack-plugin": "4.2.1",
    "core-js": "2.5.1",
    "coveralls": "2.13.2",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "handlebars-loader": "1.6.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "http-server": "^0.10.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-remap-istanbul": "0.6.0",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "2.0.6",
    "material-design-lite": "1.3.0",
    "ncp": "2.0.0",
    "node-sass": "4.7.1",
    "postcss-loader": "1.3.3",
    "protractor": "5.2.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "rxjs": "5.4.3",
    "sass-loader": "6.0.6",
    "sourcemap-istanbul-instrumenter-loader": "0.2.0",
    "string-replace-loader": "1.3.0",
    "style-loader": "0.19.0",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.5.3",
    "typings": "2.1.1",
    "web-animations-js": "2.3.1",
    "webpack": "3.8.0",
    "webpack-dev-server": "2.9.4",
    "webpack-merge": "4.1.1",
    "zone.js": "0.8.18"
  },
